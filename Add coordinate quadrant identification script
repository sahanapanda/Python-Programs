x = int(input("Enter X coordinate :"))
y = int(input("Enter Y coordinate :"))

if x>0 and y>0:
  print("First Quadrant")
if x<0 and y>0:
  print("Second Quadrant")
if x<0 and y<0:
  print("Third Quadrant")
if x>0 and y<0:
  print("Fourth Quadrant")
if -100>x>100 or 100>y>100:
  print("Range exceeded")
